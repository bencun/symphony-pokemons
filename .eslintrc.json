{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi":"error",
    "brace-style": ["error", "1tbs"],
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "eol-last": ["error"],
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
